+    public static String format_DateInUTC(java.util.Date date, String pattern) {
+        if (date != null) {
+            return TalendDate.formatDateInUTC(pattern == null ? Constant.dateDefaultPattern : pattern, date);
+        } else {
+            return null;
+        }
