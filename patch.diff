+        /**
+         * Set flag characters, one of '-+#0 or a space.
+         */
+        private void setFlagCharacters() {
+            /* '-+ #0 */
+            thousands = false;
+            leftJustify = false;
+            leadingSign = false;
+            leadingSpace = false;
+            alternateForm = false;
+            leadingZeros = false;
+            for (; pos < fmt.length(); pos++) {
+                char c = fmt.charAt(pos);
+                if (c == '\'')
+                    thousands = true;
+                else if (c == '-') {
+                    leftJustify = true;
+                    leadingZeros = false;
+                } else if (c == '+') {
+                    leadingSign = true;
+                    leadingSpace = false;
+                } else if (c == ' ') {
+                    if (!leadingSign)
+                        leadingSpace = true;
+                } else if (c == '#')
+                    alternateForm = true;
+                else if (c == '0') {
+                    if (!leftJustify)
+                        leadingZeros = true;
+                } else
+                    break;
+            }
+        }
