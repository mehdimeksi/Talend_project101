+        StringBuffer result = new StringBuffer();
+
+        for (int bcl = 0; bcl < text.length(); bcl++) {
+            char carVal = text.charAt(bcl);
+            if (carVal >= 192 && carVal <= 255) {
+                String newVal = (String) map.get(carVal - 192);
